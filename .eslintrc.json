{
	"env": {
		"browser": false,
		"node": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"plugins": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-unused-vars": "warn",
		"no-console": "off",
		"semi": ["error", "always"],
		"quotes": ["error", "single"],
		"indent": ["error", 2]
	}
}
